# Django Development Configuration
# DEBUG=True (optional)
SECRET_KEY=your-secret-key-here

# Database Configuration
DBHOST=postgres
DBNAME=tea_dev
DBUSER=tea_user
DBPASSWORD=your-password
DBPORT=5432

# CORS Configuration (comma-separated origins)
CORS_ORIGIN_WHITELIST=http://localhost:3000,http://127.0.0.1:3000

# Django ALLOWED_HOSTS (comma-separated hostnames)
ALLOWED_HOSTS=localhost,127.0.0.1,eap_backend

# GitHub OAuth
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# Azure Storage (for file uploads)
AZURE_ACCOUNT_NAME=your-azure-account-name
AZURE_ACCOUNT_KEY=your-azure-account-key
